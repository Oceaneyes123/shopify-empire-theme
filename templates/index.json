    "dynamic_html_dErhpD": {
      "type": "dynamic-html",
      "custom_css": [
        ".main-container {background: url(https:\/\/cdn.shopify.com\/s\/files\/1\/0616\/5590\/8525\/files\/St_1.png?v=1700159838); background-repeat: no-repeat; background-size: cover; padding: 50px 20px; border-radius: 25px;}",
